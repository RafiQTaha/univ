    <tr>
        <td style="width:30%;border:1px solid #ddd;background:#eee;padding:5px;font-size:10px">ETABLISSEMENT</td>
        <td style="width:0.5%;"></td>
        <td style="width:69.5%;border:1px solid #ddd;background:white;padding:5px;font-size:10px">
            {{pv.annee.formation.etablissement.designation}}
        </td>
    </tr>
    <tr>
        <td style="width:24%;border:1px solid #ddd;background:#eee;padding:5px;font-size:10px">ANNEE - SEMESTRE</td>
        <td style="width:0.5%;"></td>
        <td style="width:69.5%;border:1px solid #ddd;background:white;padding:5px;font-size:10px">
            {{pv.annee.designation}} - {{pv.semestre.designation}}
        </td>
    </tr>
    <tr>
        <td style="width:24%;border:1px solid #ddd;background:#eee;padding:5px;font-size:10px">COORDONNATEUR DE L'ANNEE</td>
