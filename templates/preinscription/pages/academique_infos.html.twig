<div class="col-md-6" style="border-right: 1px solid #eee;">
    <fieldset>
        <div class="col-md-12 row">
            <div class="col-md-6">
                <div class="form-group">
                    <label>  C.N.E (*) :</label>
                    <input class="form-control" type="text" name="cne" placeholder="C.N.E" value=""> 
                </div>
            </div> 
            <div class="col-md-6">
                <div class="form-group">
                    <label>Académie  :</label>
                    <select class="form-control chosen-select" name="id_academie">
                        <option value="">Choix Académie</option>
                        <option selected="" value="aca00000012">Rabat-Salé-Zemmour-Zaër</option>
                        <option value="aca00000013">Souss-Massa-Dra</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="col-md-12 row">
            <div class="col-md-6">
                <div class="form-group">
                    <label>filière  :</label>
                    <select class="form-control chosen-select" name="id_filiere">
                        <option value="">Choix Filière</option>
                    </select>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label>établissement  :</label>
                    <input class="form-control" type="text" name="etablissement" placeholder="Etablissement" value=""> 
                </div>
            </div>
        </div>
        <div class="col-md-12 row">
            <div class="col-md-6">
                <div class="form-group">
                    <label>Type de Bac :</label>
                    <select class="form-control chosen-select" name="id_type_bac">
                        <option value="">Choix Type</option>
                    </select>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label>  Année Bac  :</label>
                    <input class="form-control" type="text" name="annee_bac" placeholder="Année Bac" value="2015"> 
                </div>
            </div>
        </div>
        <div class="col-md-12 row">
            <div class="col-md-6">
                <div class="form-group">
                    <label>  moyenne_bac  :</label>
                    <input class="form-control" type="text" name="moyenne_bac" placeholder="Moyen Bac" value="15.4"> 
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label>  Observation :</label>
                    <textarea class="form-control" name="obs" style="width: 100%; height: 32px;"></textarea>
                </div>
            </div>
        </div>  
    </fieldset>
</div>
<div class="col-md-6 parents-tab" style="border-left: 1px solid #eee;">
    <fieldset>
        <div class="col-md-12 row">
            <div class="col-md-6">
                <div class="form-group">
                    <label> catégorie préinscription  :</label>
                    <input class="form-control" type="text" name="categorie_preinscription" placeholder="Catégorie Préinscription" value=""> 
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label> frais préinscription :</label>
                    <input class="form-control" type="text" name="frais_preinscription" placeholder="Rrais préinscription" value="0"> 
                </div>
            </div> 
        </div>
        <div class="col-md-12 row">         
            <div class="col-md-6">
                <div class="form-group">
                    <label>  langue concours  :</label>
                    <select class="form-control chosen-select" name="langue_concours">
                        <option value="">Choix Établissement</option>
                    </select>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label>  Concours  :</label>
                    <input class="form-control" type="text" name="concours_medbup" placeholder="Concours" value=""> 
                </div>
            </div>
        </div>
    </fieldset>      
</div> 

