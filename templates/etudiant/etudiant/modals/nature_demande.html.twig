<div class="modal" role="dialog" id="natureDemandemodal" aria-modal="true"> 
<div class="modal-dialog modal-md" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Nature Demande</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"> 
        <span aria-hidden="true">×</span>
        </button>
      </div>
      <div class="modal-body">
        {# <div class="alert alert-info">Veuillez selectionner un ligne Pour Continuer Cette opération !!</div> #}
          <form action="" class="form-natureDemande">
            <div class="nature">
              <label for="nature">Nature De Demande:</label>
              <select class="form-control" name="nature" id="nature">
                <option value="" disabled selected>Choix Nature De Demande</option>
                {% for natureDemande in natureDemandes %}
                    <option value="{{natureDemande.id}}">{{natureDemande.designation}}</option>
                {% endfor %}
              </select>
            </div>
            <div class="sousNature">
              <label for="sousNature">Sous Nature Demande:</label>
              <select class="form-control" name="sousNature" id="sousNature">
              </select>
            </div>
            
            <div class="d-flex mt-5 mb-5">
              {# <button class="btn btn-primary ml-auto">G.Preinscription</button> #}
              <button type="submit" class="btn btn-success ml-auto" id="enregistrer" disabled> <i class="fas fa-check-circle"></i> Enregistrer</button>
            </div>
          </form>
