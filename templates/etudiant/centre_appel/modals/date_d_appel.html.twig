<div class="modal" tabindex="-1" role="dialog" id="date-d-appel-modal" aria-modal="true"> 
<div class="modal-dialog modal-md" style="max-width: 400px !important;" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Date Dernier Appel</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"> 
        <span aria-hidden="true">×</span>
        </button>
      </div>
      <div class="modal-body">
        <!-- <div class="alert alert-info">Veuillez selectionner un ligne Pour Continuer Cette opération !!</div> -->
        <form id="date_appele_save">
          <label for="">Date</label>
          <input class="form-control" type="date" name="dateappelle" id="dateappelle">
          <label for="">Premier RDV</label>
          <input class="form-control" type="date" name="rdv1" id="rdv1">
          <label for="">Deusieme RDV</label>
          <input class="form-control" type="date" name="rdv2" id="rdv2">
          <label> Statut D'Appel :</label>
          <select class="form-control chosen-select" name="statut_appel" id ='statut_appel'>
              <option value="" selected>Choix Statut D'Appel</option>
              <option value="DECROCHE">DECROCHE</option>
              <option value="BV">BV</option>
              <option value="NRP">NRP</option>
              <option value="FN">FN</option>
              <option value="PAS DE NUMERO">PAS DE NUMERO</option>
          </select>
          <label> Observation :</label>
          <select class="form-control chosen-select" name="Observation" id ='Observation'>
              <option value="" selected>Choix Observation</option>
              <option value="1ER ANNEE">1ER ANNEE</option>
              <option value="TRANSFERT">TRANSFERT</option>
              <option value="PASSERELLE">PASSERELLE</option>
              <option value="NON INTERESSE">NON INTERESSE</option>
          </select>
          <button type="submit" class="btn btn-success mt-2"><i class="fas fa-check-circle"></i> Enregistrer</button>
        </form>
      </div>
    </div>
  </div>
</div>