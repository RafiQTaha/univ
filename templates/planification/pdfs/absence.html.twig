                    <td style="padding:6px;border-right:1px solid white;text-align: left;">{{inscription.admission.preinscription.etudiant.nom}}</td>
                    <td style="padding:6px;border-right:1px solid white;text-align: left;">{{inscription.admission.preinscription.etudiant.prenom}}</td>
                    <td style="padding:6px;border-right:1px solid white;text-align: left;">  </td>
                    <td style="padding:6px;text-align: left;">  </td>
                </tr>
            {% set key = key + 1 %}
            {% endfor %}      
        </tbody>
    </table>

